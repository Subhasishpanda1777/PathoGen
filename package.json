{
  "name": "pathogen-monorepo",
  "version": "1.0.0",
  "description": "PathoGen - Public Health Monitoring Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./packages/*\" dev",
    "build": "pnpm --recursive --filter \"./packages/*\" build",
    "lint": "pnpm --recursive --filter \"./packages/*\" lint",
    "test": "pnpm --recursive --filter \"./packages/*\" test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.5.0",
    "tailwind-merge": "^3.4.0"
  }
}

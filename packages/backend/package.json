{
  "name": "@pathogen/backend",
  "version": "1.0.0",
  "description": "PathoGen Backend API Server",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pathogen-server": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "seed": "node scripts/seed-large-scale-data.js",
    "seed:sample": "node scripts/seed-sample-data.js",
    "seed:dashboard": "node scripts/seed-dashboard-data.js",
    "seed:prevention": "node scripts/seed-prevention-measures.js",
    "create:rewards-tables": "node scripts/create-rewards-tables.js",
    "scrape:medicines": "node scripts/scrape-medicines.js",
    "check-port": "node scripts/check-port.js",
    "kill-port": "node scripts/check-port.js 5000 --kill",
    "migrate:location": "node scripts/add-location-fields.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "drizzle-kit": "^0.20.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "@sentry/node": "^7.91.0",
    "nodemailer": "^6.9.7",
    "puppeteer": "^21.6.1",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^6.4.14",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0"
  }
}

